{
  "name": "react-vite-tailwindcss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "fmt": "prettier --write \"./**/*.{js,jsx,css,scss}\" --plugin=prettier-plugin-tailwindcss",
    "tailwind": "tailwindcss -i ./src/main.css -o ./public/main.css --watch",
    "postcss": "postcss ./src/main.css -o ./public/main.css --watch",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@cloudinary/react": "^1.12.0",
    "@cloudinary/url-gen": "^1.16.1",
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "2.1.1",
    "@types/mapbox-gl": "^2.7.20",
    "graphql": "^16.8.1",
    "mapbox-gl": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.7",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12"
  },
  "prettier": {
    "trailingComma": "all",
    "bracketSpacing": true,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 1024
  }
}
